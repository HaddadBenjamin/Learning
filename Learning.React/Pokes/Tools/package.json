{
  "name": "lazyload",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.17.0"
  },
  "scripts": {
    "dev": "concurrently \"yarn dev:react\" \"yarn dev:apiMock\"",
    "dev:react": "react-scripts start",
    "dev:apiMock": "nodemon src/devTools/server/server.ts --config src/devTools/server/nodemon.json",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:sass": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "test": "react-scripts test",
    "test:coverage": "ts-node -r tsconfig-paths/register --project src/shared/configuration/tsconfig.json --transpile-only scripts/jest/runTestsWithCoverage.ts",
    "test:showConfig": "npm test -- --showConfig",
    "check:arrayExtensions": "ts-node -r tsconfig-paths/register --project src/shared/configuration/tsconfig.json --transpile-only src/shared/extensions/arrayExtensions.ts",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 100,
        "lines": 100,
        "statements": 0
      },
      "src/shared/redux": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/devTools": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.19",
    "@types/react-router-dom": "^5.3.1",
    "@types/redux-saga": "^0.10.5",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "express-oas-generator": "^1.0.41",
    "mongoose": "^6.0.11",
    "mongoose-to-swagger": "^1.3.0",
    "node-cmd": "^5.0.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.11",
    "postcss-scss": "^4.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "stylelint": "^14.0.0",
    "stylelint-config-standard-scss": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
